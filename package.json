{
  "name": "@soybeanjs/vue-admin-layout",
  "version": "1.0.4",
  "description": "a layout component of admin template, support vue2 and vue3",
  "private": false,
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "xia:dev": "pnpm run -C xia dev",
    "xia:build": "pnpm run -C xia build",
    "dev": "vite",
    "build": "npm run typecheck && vite build",
    "build:vercel": "cross-env VITE_IS_VERCEL=1 vite build",
    "typecheck": "vue-tsc --noEmit",
    "release": "standard-version",
    "pub": "npm publish --access=public",
    "preview": "vite preview",
    "lint": "eslint --fix ./ --ext .vue,.js,jsx,.ts,tsx",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asialiugf/soybean-admin-layout.git"
  },
  "keywords": [
    "admin",
    "layout",
    "admin-layout",
    "vue2",
    "vue3"
  ],
  "author": "asialiugf",
  "license": "MIT",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "dependencies": {
    "@unocss/reset": "^0.33.5",
    "colord": "^2.9.2",
    "css-render": "0.15.9",
    "vue-demi": "^0.12.5"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.4.6",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.16.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.2",
    "prettier": "^2.6.2",
    "typescript": "^4.7.2",
    "unocss": "^0.33.5",
    "vite": "2.9.8",
    "vite-plugin-dts": "^1.2.0",
    "vue": "^3.2.36",
    "vue-tsc": "^0.34.16"
  }
}
