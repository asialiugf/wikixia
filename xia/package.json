{
  "name": "@asialine/wikixia",
  "version": "0.0.3",
  "description": "am-editor component for vue",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/red-axe/am-editor-vue.git"
  },
  "main": "index.ts",
  "scripts": {
    "dev": "vite --mode development",
    "build": "vite build",
    "preview": "vite preview",
    "release": "vue-tsc --noEmit && vite build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "format": "prettier --write \"src/**/*.vue\"",
    "lint-fix": "eslint --fix --color --ext .js,.vue,.ts src",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "author": "byoungd",
  "license": "MIT",
  "dependencies": {
    "@vitejs/plugin-vue": "~2.2.0",
    "eslint": "^7.2.31",
    "keymaster": "^1.6.2",
    "less": "~4.1.2",
    "less-loader": "~10.2.0",
    "lodash": "^4.17.21",
    "prettier": "~2.5.1",
    "tinycolor2": "^1.4.2",
    "vite": "~2.8.1",
    "vite-plugin-pages": "~0.20.2",
    "vite-plugin-vue-layouts": "~0.6.0",
    "vite-plugin-vue-setup-extend": "~0.4.0",
    "vite-plugin-windicss": "~1.7.0",
    "vue-router": "^4",
    "vue-tsc": "~0.31.4",
    "windicss": "~3.4.3"
  },
  "devDependencies": {
    "@types/keymaster": "^1.6.28",
    "@types/lodash": "^4.14.178",
    "@types/tinycolor2": "~1.4.3",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/compiler-sfc": "^3.2.9",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.1",
    "prettier-eslint-cli": "^5.0.1"
  }
